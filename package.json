{
	"name": "g-translate",
	"displayName": "G-Translate",
	"description": "Google Translate for vscode.",
	"version": "0.0.1",
	"publisher": "maple3142",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.translate"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.translate",
				"title": "Translate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"google-translate-api": "^2.3.0"
	}
}
